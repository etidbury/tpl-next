{
    "name": "tpl-next",
    "version": "0.1.6",
    "description": "",
    "main": "index.js",
    "engines": {
        "node": "10.x.x"
    },
    "scripts": {
        "deploy": "now",
        "flow": "$(npm bin)/flow",
        "eslint": "$(npm bin)/eslint --max-warnings 300 ./",
        "next": "$(npm bin)/next",
        "setup": "tidil setup --tpl next",
        "start": "NODE_ENV=production node -r dotenv/config index.js",
        "test": "npm run flow && npm run eslint",
        "test:ci": "npm run test",
        "dev": "npm run next",
        "prod": "npm run start",
        "build": "$(npm bin)/next build",
        "now-build": "$(npm bin)/next build && npm run test:ci",
        "build:reset": "rm -rf .build*",
        "storybook": "$(npm bin)/start-storybook -p $(tidil free-port) -c config/storybook",
        "storybook:build": "$(npm bin)/build-storybook -c config/storybook -o storybook"
    },
    "dependencies": {
        "@storybook/cli": "^3.4.11",
        "axios": "0.18.0",
        "cors": "^2.8.4",
        "dotenv": "6.0.0",
        "fastify": "1.9.0",
        "fastify-blipp": "1.2.1",
        "fastify-compress": "0.6.0",
        "fastify-helmet": "1.0.3",
        "next": "^7.0.1",
        "react": "16.4.1",
        "react-dom": "16.4.1",
        "react-ga": "^2.5.3",
        "react-redux": "5.0.6",
        "redux": "3.6.0",
        "redux-thunk": "2.3.0",
        "tpl-next-helpers": "^0.1.0"
    },
    "devDependencies": {
        "@babel/plugin-proposal-optional-chaining": "^7.0.0",
        "@storybook/react": "3.4.10",
        "autoprefixer": "9.0.2",
        "babel-eslint": "8.2.6",
        "babel-loader": "7.1.5",
        "babel-plugin-module-resolver": "^3.1.1",
        "babel-plugin-transform-define": "^1.3.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "eslint": "^5.3.0",
        "eslint-cli": "^1.1.1",
        "eslint-plugin-flowtype": "^2.50.0",
        "eslint-plugin-promise": "^3.8.0",
        "flow-bin": "^0.82.0",
        "node-sass": "4.9.2",
        "postcss-easy-import": "3.0.0",
        "postcss-loader": "2.1.6",
        "raw-loader": "0.5.1",
        "redux-devtools-extension": "2.13.5",
        "redux-logger": "3.0.6",
        "sass-loader": "7.1.0"
    }
}
